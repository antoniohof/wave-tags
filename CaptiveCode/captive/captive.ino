String readFile () {
    String data;

    int i;
    
    //Read File data
    File f = SPIFFS.open(filename, "r");
    
    if (f)
    {
        while(f.available()) {
          //Lets read line by line from the file
          String line = f.readString();
          data += line;
        }
        f.close();  //Close file
        return data;
    }
}

